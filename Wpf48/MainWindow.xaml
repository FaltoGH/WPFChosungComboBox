<Window x:Class="Wpf48.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf48"
        xmlns:ccb="clr-namespace:ChosungComboBox;assembly=ChosungComboBox"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450"
        Background="{x:Static SystemColors.ActiveCaptionTextBrush}">
    <Grid Margin="9">
        <StackPanel Margin="9">
            <ccb:CComboBox x:Name="ccb" EnterKeyDown="ccb_EnterKeyDown" Margin="9"/>
            <Button Click="Button_Click" Margin="9">test</Button>
        </StackPanel>
    </Grid>
</Window>
